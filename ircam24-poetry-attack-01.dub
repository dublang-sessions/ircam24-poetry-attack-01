hooks('espeak', {
  function(plugin, block)
    -- map sound name to video number
    local map = {
      questions = 16,
      answers = 14,
      how = 15,
      black = 13,
      blue = 12,
      red = 11,
    }
    -- parse espeak code block
    local video = 0
    for k, value in pairs(map) do
      if block:match(k) then
        video = value
      end
    end
    if video > 0 then
      mpv(string.format([[video "%02d.mp4"]], video))
      if video == 4 then
        mpv([[clear]])
        mpv([[hue $ h 230 $ s 9]])
      elseif video == 5 then
