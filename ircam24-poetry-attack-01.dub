hooks('tidalcycles', {
  -- hook function for tidalcycles
  -- hook to control video playback
  function(plugin, block)
    -- parse tidal code # begin <n>
    local begin = block:match("# begin (%d+%.?%d*)")
    if not begin then
      begin = 0
    end
    local begin = begin * 100
    -- map sound name to video number
    local map = {
      bd = 2,
      mocreo = 4,
      bass2 = 6,
      superpiano = 8,
      supersiren = 9,
      cp = 7,
      jazz = 5,
      hh27 = 3,
      hh = 10,
      pandeiro = 11,
      berimbau = 12,
    }
    -- parse tidal code block
    local video = 0
    for k, value in pairs(map) do
      if block:match(k) then
        local seed = math.random(3) - 1
        video = value + seed
      end
    end
    if video > 0 then
      if video ~= playing then
        mpv(string.format([[video "%s/%02d.mp4" %s%%]], dirs[math.random(#dirs)], video, begin))
        playing = video
      end
    else
      obs(string.format([[scene.set %s]], scenes[math.random(#scenes)]))
      mpv([[clr]])
      mpv(string.format([[video "%s/%02d.mp4" %s%%]], dirs[math.random(#dirs)], 16, begin))
      vim.notify(string.format([[video "%s/%02d.mp4" %s%%]], dirs[math.random(#dirs)], 16, begin))
      playing = nil
    end
  end,
  -- hook to control video speed
  function(plugin, block)
    -- parse tidal code # speed <n>
    local speed = block:match("# speed (%d+%.?%d*)")
    if not speed or tonumber(speed) < 0 then
      speed = 1
    end
    speed = tonumber(speed)
    mpv(string.format([[set speed %s]], speed))
  end,
  -- hook to control video shuffle from chop or gap tidal functions
  function(plugin, block)
    -- parse tidal code # speed <n>
    local chop = block:match("$ chop (%d+)")
    if not chop then
      chop = block:match("$ gap (%d+)")
      if not chop then
        chop = 1
      end
    end
    chop = tonumber(chop)
    local map = {
      [1] = "0",
      [2] = "1 0",
      [3] = "0 2 1",
      [4] = "0 3 1 2",
      [5] = "0 4 2 2 3",
      [6] = "1 5 3 2 4 0",
      [7] = "0 6 3 4 2 5 1",
      [8] = "0 6 3 4 2 5 1",
      [9] = "0 6 3 4 2 5 1 7",
      [10] = "8 6 3 0 2 5 1 7 3",
      [11] = "0 6 8 4 2 9 1 7 0 0",
    }
    mpv(string.format([[vf add "@chop:shuffleframes:%s"]], map[chop]))
  end,
  function(plugin, block)
    if block:match("bass2") then
      local thr = math.random(50)
      local fac = math.random(100)
      local low = math.random(50)
      mpv(string.format([[amp $ fac %d $ low %d $ thr %d]], fac, low, thr))
    end
    if block:match("superpiano") then
      nvimqt([[noise(' - _ ')]])
    elseif block:match("pandeiro") then
      nvimqt([[noise('  *    ')]])
    else
      nvimqt([[fill(' - _ ')]])
    end
  end,
  -- hook to control video edge from squiz tidal function
  function(plugin, block)
    -- parse tidal code # squiz <n>
    local squiz = block:match("# squiz (%d+%.?%d*)")
    if not squiz then
      squiz = 0
    end
    squiz = tonumber(squiz)
    if squiz > 1 and squiz < 1.5 then
      mpv([[edge $ mod colormix]])
    elseif squiz >= 1.5 and squiz < 2.5 then
      mpv([[edge $ mod wires]])
    elseif squiz >= 2.5 then
      mpv([[edge $ mod canny]])
    end
  end,
  -- hook to control video shufflepixels from squiz tidal function
  --function(plugin, block)
